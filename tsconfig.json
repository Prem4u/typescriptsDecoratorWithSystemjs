{   
    "compilerOptions": {
    "declaration": false,
    "emitDecoratorMetadata": false,
    "experimentalDecorators": false,
    "module": "system",
    "moduleResolution": "node",
    "outFile": "./out/main2.js",
    "noFallthroughCasesInSwitch": false,
    "noImplicitAny": false,
    "noImplicitReturns": false,
    "removeComments": false,
    "sourceMap": false,
    "strictNullChecks": false,
    "target": "es5",
    "outDir":"out",
    "types": ["node"],
	"skipLibCheck": false
  },
    "compileOnSave": true,
	"buildOnSave": true
}